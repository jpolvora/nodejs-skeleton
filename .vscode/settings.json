{
  "eslint.autoFixOnSave": true,
  "eslint.enable": true,
  "eslint.alwaysShowStatus": true,
  "eslint.run": "onSave",
  "eslint.lintTask.enable": true,
  "eslint.packageManager": "npm",
  "eslint.validate": [
    "javascript",
    "javascriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "editor.formatOnSave": true,
  "javascript.format.enable": false
}